type StockAttribute {
  id: ID!
  ticker: String!
  name: String!
}

type StockDateAndPrice {
  id: ID!
  ticker: String!
  stockDate: String!
  open: String!
  high: String!
  low: String!
  close: String!
  adjustedClose: String!
  volume: String!  
}

type Query {
	allStockAttributes: [StockAttribute]
	allStockDateAndPrice: [StockDateAndPrice]
}

type Mutation {
  modifyStockAttribute(ticker: String!, name: String!): StockAttribute
}

schema {
	query: Query
	mutation: Mutation
}

